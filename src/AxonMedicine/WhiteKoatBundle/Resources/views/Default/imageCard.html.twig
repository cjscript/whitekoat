<div class="card imageCard">
	<div class="cardContent">
		<div class="topBar">
			<div class="topBarPadding"></div>
			<div class="cardTitle">
				<h1><small>Images of </small><br>{{ libValue.name }}</h1>
			</div>
			<div class="topRightButtons">
				<div class="buttonTitle">&nbsp;</div>
				<a href="#" class="closeButton" data-tooltip="Close"></a>
			</div>
		</div>
		<div class="spacer"></div>
		<div class="cardTabContainer">
			<div class="imageContent">
				{% for image in images %}
					<a href="../images_3921343193822/{{ image.imageref.id }}.{{ image.imageref.originalfileext }}" data-lightbox="{{ libValue.name }}"><img src="../images_3921343193822/{{ image.imageref.id  }}_thmb.{{ image.imageref.originalfileext  }}"></a>
					{% if (loop.index is divisibleby(2)) and not loop.last %}
						<br>
					{% endif %}
				{% endfor %}
			</div>
		</div>
	</div>
</div>