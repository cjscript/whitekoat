{%extends "AxonMedicineWhiteKoatBundle:Default:student.drug.disease.without.menu.html.twig"%}

{%block libcontent %}

<div id="someclass">
    <fieldset id="diseaserelationships">
        <legend>Search Results</legend>

{% if diseaseSearchOutput is defined %}
        <div>
            <table class="table table-striped table-bordered">
                <thead bgcolor="#cccccc">
                    <tr>
                        <th></th>
                    </tr>
                </thead>     
                <tbody>
            {%for something in diseaseSearchOutput %}
                    <tr>
                        <td>{{something}}</td>
                    </tr>
             {%endfor%}
                </tbody>
            </table>
        </div>
{%endif%}

        <div style="padding-top:30px">
            <input class="btn btn-primary btn-lg" type="submit" value="Close Window" onclick="window.close();
                    return true;">
        </div>
</div>
{%endblock%}

{%block tablecontent %}
{%endblock%}
